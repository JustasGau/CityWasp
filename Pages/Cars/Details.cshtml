        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Car.manufacturer)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Car.coordinates)
        </dt>
        <dd class="col-sm-10">
